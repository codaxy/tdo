{
  "name": "tdo",
  "version": "1.0.0",
  "description": "Electron wrapper for tdo",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . tdo --out=dist --overwrite --platform=win32 --arch=ia32 --icon=assets/app-icon/ico/favicon.ico",
    "dev": "electron . --debug",
    "setup": "electron-installer-windows --src dist/tdo-win32-ia32/ --dest dist/installers/"
  },
  "author": "Nebojsa Peric",
  "license": "MIT",
  "devDependencies": {
    "electron": "^5.0.2",
    "electron-packager": "^13.1.1"
  }
}
